  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug",
      "type": "node",
      "program": "./bin/ex.js",
      "request": "launch",
      "env": {
        "DEBUG": "true"
      },
      "skipFiles": [
        "<node_internals>/**"
      ],
    },
    {
      "name": "quick-Launch",
      "type": "node",
      "program": "${file}",
      "request": "launch",
      "env": {
        "DEBUG": "true"
      },
      "skipFiles": [
        "<node_internals>/**"
      ],
    },
  ]
}
